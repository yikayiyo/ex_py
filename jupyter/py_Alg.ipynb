    "def selection_sort(nums):\n",
    "    length = len(nums)\n",
    "    for i in range(length):\n",
    "        m = nums.index(min(nums[i:]))\n",
    "        if m!=i:\n",
    "            nums[i],nums[m]=nums[m],nums[i]\n",
    "    return nums\n",
    "array=[5,3,6,2,10,9]\n",
    "res=selection_sort(array)\n",
    "print(res)"
